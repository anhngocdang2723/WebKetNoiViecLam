                                                    <table class="table-style3 table at-savesearch">
                                                        <thead class="t-head">
                                                            <tr>
                                                                <th scope="col">TIÊU ĐỀ</th>
                                                                <th scope="col">DANH MỤC</th>
                                                                <th scope="col">LOẠI/GIÁ TIỀN</th>
                                                                <th scope="col">HÀNH ĐỘNG</th>
                                                            </tr>
                                                        </thead>

                                                        @await Component.InvokeAsync("UserManageService_DangThucHien")

                                                    </table>
                                                </div>
                                            </div>
                                            <div class="tab-pane fade" id="nav-item4" role="tabpanel" aria-labelledby="nav-item4-tab">
                                                <div class="packages_table table-responsive">
                                                    <table class="table-style3 table at-savesearch">
                                                        <thead class="t-head">
                                                            <tr>
                                                                <th scope="col">TIÊU ĐỀ</th>
                                                                <th scope="col">DANH MỤC</th>
                                                                <th scope="col">LOẠI/GIÁ TIỀN</th>
                                                                <th scope="col">HÀNH ĐỘNG</th>
                                                            </tr>
                                                        </thead>

                                                        @await Component.InvokeAsync("UserManageService_DaHoanThanh")

                                                    </table>
                                                </div>
                                            </div>
                                            <div class="tab-pane fade" id="nav-item5" role="tabpanel" aria-labelledby="nav-item5-tab">
                                                <div class="packages_table table-responsive">
                                                    <table class="table-style3 table at-savesearch">
                                                        <thead class="t-head">
                                                            <tr>
                                                                <th scope="col">TIÊU ĐỀ</th>
                                                                <th scope="col">DANH MỤC</th>
                                                                <th scope="col">LOẠI/GIÁ TIỀN</th>
                                                                <th scope="col">HÀNH ĐỘNG</th>
                                                            </tr>
                                                        </thead>

                                                        @await Component.InvokeAsync("UserManageService_KDuocDuyet")

                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <footer class="dashboard_footer pt30 pb30">
                        <div class="container">
                            <div class="row align-items-center justify-content-center justify-content-md-between">
                                <div class="col-auto">
                                    <div class="copyright-widget">
                                        <p class="mb-md-0">© Freeio. 2023 CreativeLayers. All rights reserved.</p>
                                    </div>
                                </div>
                                <div class="col-auto">
                                    <div class="footer_bottom_right_btns at-home8 text-center text-lg-end">
                                        <ul class="p-0 m-0">
                                            <li class="list-inline-item bg-white">
                                                <select class="selectpicker show-tick">
                                                    <option>US$ USD</option>
                                                    <option>Euro</option>
