    <![endif]-->
</head>
<body>
    <div class="wrapper">
        <div class="preloader"></div>

        <!-- Main Header Nav -->
        <header class="header-nav nav-innerpage-style menu-home4 dashboard_header main-menu">
            <!-- Ace Responsive Menu -->
            <nav class="posr">
                <div class="container-fluid pr30 pr15-xs pl30 posr menu_bdrt1">
                    <div class="row align-items-center justify-content-between">
                        <div class="col-6 col-lg-auto">
                            <div class="text-center text-lg-start d-flex align-items-center">
                                <div class="dashboard_header_logo position-relative me-2 me-xl-5">
                                    <a href="index.html" class="logo"><img src="~/images/header-logo-dark.svg" alt=""></a>
                                </div>
                                <div class="fz20 ml90">
                                    <a href="#" class="dashboard_sidebar_toggle_icon vam"><img src="~/images/dashboard-navicon.svg" alt=""></a>
                                </div>
                                <a class="login-info d-block d-xl-none ml40 vam" data-bs-toggle="modal" href="#exampleModalToggle" role="button"><span class="flaticon-loupe"></span></a>
                                <div class="ml40 d-none d-xl-block">
                                    <div class="search_area dashboard-style">
                                        <input type="text" class="form-control border-0" placeholder="What service are you looking for today?">
                                        <label><span class="flaticon-loupe"></span></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>
        </header>
        <!-- Search Modal -->

        <!-- Mobile Nav  -->

        <div class="dashboard_content_wrapper">
            <div class="dashboard dashboard_wrapper pr30 pr0-xl">
                <div class="dashboard__sidebar d-none d-lg-block">
                    <div class="dashboard_sidebar_list">
                        <p class="fz15 fw400 ff-heading pl30">Start</p>
                        <div class="sidebar_list_item">
                            <a href="~/HomeWeb" class="items-center"><i class="flaticon-home mr15"></i>Trang chủ</a>
                        </div>
                        <div class="sidebar_list_item">
                            <a href="page-dashboard-save.html" class="items-center"><i class="flaticon-like mr15"></i>Saved</a>
                        </div>
                        <div class="sidebar_list_item ">
                            <a href="page-dashboard-message.html" class="items-center"><i class="flaticon-chat mr15"></i>Message</a>
                        </div>
                        <div class="sidebar_list_item ">
                            <a href="page-dashboard-reviews.html" class="items-center"><i class="flaticon-review-1 mr15"></i>Reviews</a>
                        </div>
                        <div class="sidebar_list_item">
                            <a href="page-dashboard-invoice.html" class="items-center"><i class="flaticon-receipt mr15"></i>Invoice</a>
                        </div>
                        <div class="sidebar_list_item">
                            <a href="page-dashboard-payouts.html" class="items-center"><i class="flaticon-dollar mr15"></i>Payouts</a>
                        </div>
                        <p class="fz15 fw400 ff-heading pl30 mt30">Tổ chức và quản lý</p>
                        <div class="sidebar_list_item ">
                            <a href="~/UserDashboard/ManageService" class="items-center -is-active"><i class="flaticon-presentation mr15"></i>Quản lý dịch vụ</a>
                        </div>
                        <div class="sidebar_list_item ">
                            <a href="~/UserDashboard/ManageJob" class="items-center "><i class="flaticon-briefcase mr15"></i>Quản lý công việc</a>
                        </div>
                        <div class="sidebar_list_item ">
                            <a href="/Blogs/Create" class="items-center"><i class="flaticon-content mr15"></i>Đăng bài</a>
                        </div>
                        <p class="fz15 fw400 ff-heading pl30 mt30">Tài khoản</p>
                        <div class="sidebar_list_item ">
                            <a href="https://localhost:7117/Identity/Account/Manage" class="items-center"><i class="flaticon-photo mr15"></i>My Profile</a>
                        </div>
                        <div class="sidebar_list_item ">
                            <a href="https://localhost:7117/Identity/Account/Manage/ChangePassword" class="items-center"><i class="flaticon-photo mr15"></i>Đổi mật khẩu</a>
                        </div>
                        <div class="sidebar_list_item ">
                            <a href="https://localhost:7117/Identity/Account/Login" class="items-center"><i class="flaticon-logout mr15"></i>Đăng xuất</a>
                        </div>
                    </div>
                </div>
                <div class="dashboard__main pl0-md">
                    <div class="dashboard__content hover-bgc-color">
                        <div class="row pb40">
                            <div class="col-lg-12">
                                <div class="dashboard_navigationbar d-block d-lg-none">
                                </div>
                            </div>
                            <div class="col-lg-9">
                                <div class="dashboard_title_area">
                                    <h2>QUẢN LÝ DỊCH VỤ</h2>
                                    <p class="text">Đăng tải hoặc quản lý dịch vụ đã đăng</p>
                                </div>
                            </div>
                            <div class="col-lg-3">
                                <div class="text-lg-end">
                                    <a href="~/Services/Create" class="ud-btn btn-dark default-box-shadow2">Thêm dịch vụ<i class="fal fa-arrow-right-long"></i></a>
                                </div>
                            </div>
                        </div>
